<div class="panel panel-warning">
  <div class="panel-heading" style="height: 75px;">
    <div class="row">
      <div class="col-xs-3">
        <%= image_tag 'ops-timezones.png', class: 'scoreboxicon' %>
      </div>
      <div class="col-xs-9 text-right">
        <div class="huge"><%= 10 %></div>
        <div><b>Timezones</b> travelled</div>
      </div>
    </div>
  </div>
  <div class="panel-footer">
    <span class="pull-left">Previous week: <%= 5 %> &nbsp; <i class="fa fa-arrow-circle-up"></i></span>
    <span class="pull-right"><i class="fa fa-info-circle"></i></span>
    <div class="clearfix"></div>
  </div>
</div>
