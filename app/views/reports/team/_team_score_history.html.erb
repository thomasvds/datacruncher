<div class="panel panel-default">

  <%= render 'reports/shared/panel_heading', modal_id: "modal-team-history", collapse_id: "collapse-team-history", title: "TEAM HISTORY", explanation: "This chart shows the evolution of weekly individual sustainability scores and the corresponding average for all team members." %>

  <div class="collapse in" id="collapse-team-history">
    <div class="panel-body">
      <%= render 'reports/charts/chart_score_history', chart_id: "team_chart", title: '', yaxistitle: 'Weekly sustainability score', xaxis: @list_of_weeks, data: @team_score_by_week %>
    </div>
  </div>
</div>
