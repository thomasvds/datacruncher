<div class="row">
  <% @score_box_data.each do |key, value| %>
  <div class="col-lg-3 col-md-6">
    <div class="panel panel-<%= key %>">
      <div class="panel-heading" style="height: 75px;">
        <div class="row">
          <div class="col-xs-3">
            <% icon_name = key + '-icon.png' %>
            <%= image_tag icon_name, class: 'scoreboxicon' %>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= value['last_week_count'] %></div>
            <div>People in <b><%= key %></b> zone</div>
          </div>
        </div>
      </div>
      <div class="panel-footer">
        <span class="pull-left">Previous week: <%= value['week_before_last_count'] %></span>
        <span class="pull-right"><i class="fa fa-arrow-circle-<%= value['evolution'] %>"></i></span>
        <div class="clearfix"></div>
      </div>
    </div>
  </div>
  <% end %>
</div>
